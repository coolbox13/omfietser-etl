{
  "name": "supermarket-processor",
  "version": "5.0",
  "description": "Processor for supermarket product data",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc && cp src/config/default.json dist/config/",
    "build:all": "tsc && cp src/config/default.json dist/config/",
    "start": "ts-node src/index.ts",
    "start:api": "ts-node src/api/index.ts",
    "start:cli": "ts-node src/cli/index.ts",
    "start:dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "start:api:dev": "ts-node-dev --respawn --transpile-only src/api/index.ts",
    "start:api:legacy": "ts-node research/api/index.ts",
    "start:api:legacy:dev": "ts-node-dev --respawn --transpile-only research/api/index.ts",
    "process": "npm run build && node dist/index.js",
    "api": "npm run build && node dist/api/index.js",
    "cli": "npm run build && node dist/cli/index.js",
    "validate": "npm run build && node dist/scripts/validate-data.js",
    "visualize": "npm run build && node dist/scripts/visualize-data.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "chalk": "^5.4.1",
    "cli-progress": "^3.12.0",
    "commander": "^13.1.0",
    "dotenv": "^17.2.2",
    "express": "^4.21.2",
    "fast-levenshtein": "^3.0.0",
    "fs-extra": "^11.3.0",
    "lodash": "^4.17.21",
    "pg": "^8.16.3",
    "uuid": "^9.0.1",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@types/cli-progress": "^3.11.6",
    "@types/express": "^5.0.3",
    "@types/fast-levenshtein": "^0.0.4",
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^29.5.14",
    "@types/lodash": "^4.17.15",
    "@types/mocha": "^10.0.10",
    "@types/node": "^20.19.13",
    "@types/pg": "^8.15.5",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^8.18.0",
    "@typescript-eslint/parser": "^8.18.0",
    "eslint": "^9.17.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  }
}
